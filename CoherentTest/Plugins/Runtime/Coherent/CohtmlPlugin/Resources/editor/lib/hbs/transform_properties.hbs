{{#hideHummingbird env}}
    <label class="transform-label">
        Transform Origin
    </label>
    <div class="text-line"></div>
    <div class="wrap-property transform-origin-x clearfix">
        <label>
            originX:
            <div type="button" class="add-keyframe" data-property-key="transform-origin"
                 data-property-set="transform-origin">

            </div>
        </label>
        {{#if widget.transform-origin.transform-origin-x}}
            <select name="" class="units-string" data-property-set="units" data-property-key="transform-origin-x">
                {{#select_units widget.transform-origin.transform-origin-x}}
                    <option value="px">px</option>
                    <option value="%">%</option>
                {{/select_units}}
            </select>
            <input property type="number" step="{{#format_value widget.transform-origin.transform-origin-x}}{{/format_value}}"
                   value="{{#number_only widget.transform-origin.transform-origin-x}}{{/number_only}}"
                   class="units-number"
                   data-property-set="transform-origin" data-property-key="transform-origin-x"/>
        {{else}}
            <select name="" class="units-string" data-property-set="units" data-property-key="transform-origin-x">
                {{#select_units widget.transform-origin.transform-origin-x}}
                    <option value="px">px</option>
                    <option value="%" selected="selected">%</option>
                {{/select_units}}
            </select>
            <input property type="number" step="1" value="50" class="units-number" data-property-set="transform-origin"
                   data-property-key="transform-origin-x"/>
        {{/if}}
    </div>

    <div class="wrap-property transform-origin-y clearfix">
        <label>
            originY:
        </label>
        {{#if widget.transform-origin.transform-origin-y}}
            <select name="" class="units-string" data-property-set="units" data-property-key="transform-origin-y">
                {{#select_units widget.transform-origin.transform-origin-y}}
                    <option value="px">px</option>
                    <option value="%">%</option>
                {{/select_units}}
            </select>
            <input property type="number" step="{{#format_value widget.transform-origin.transform-origin-y}}{{/format_value}}"
                   value="{{#number_only widget.transform-origin.transform-origin-y}}{{/number_only}}"
                   class="units-number"
                   data-property-set="transform-origin" data-property-key="transform-origin-y"/>
        {{else}}
            <select name="" class="units-string" data-property-set="units" data-property-key="transform-origin-y">
                {{#select_units widget.transform-origin.transform-origin-y}}
                    <option value="px">px</option>
                    <option value="%" selected="selected">%</option>
                {{/select_units}}
            </select>
            <input property type="number" step="1" value="50" class="units-number" data-property-set="transform-origin"
                   data-property-key="transform-origin-y"/>
        {{/if}}
    </div>

    <span class="dismiss-transform-origin">reset</span>
{{/hideHummingbird}}
