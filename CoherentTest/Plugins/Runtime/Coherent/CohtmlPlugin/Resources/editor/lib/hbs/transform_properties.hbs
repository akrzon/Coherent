<div class="wrap-property scale-x clearfix">
    <label>
        scaleX:
        <div type="button" class="add-keyframe" data-property-key="scaleX" data-property-set="transform">

        </div>
    </label>
    <span class="fa fa-times-circle remove-property" data-type="scaleX"> </span>
    {{#if widget.transform.scaleX}}
        <input property type="number" step="0.1" min="0" maxlength="1" value="{{ widget.transform.scaleX }}"
               data-property-set="transform" data-property-key="scaleX"/>
    {{else}}
        <input property type="number" step="0.1" min="0" maxlength="1" value="1.0" data-property-set="transform"
               data-property-key="scaleX"/>
    {{/if}}
</div>

